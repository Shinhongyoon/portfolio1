}
var onload_tracking = function() {
	if (typeof wpts != 'undefined' && wpts.tag) {
		wpts.tag({
			ty : 'home',
			mud : { }
		});
	}
}
if (window.location.hostname 
		&& (window.location.hostname.indexOf('card.hyundaicard.com') >= 0 || window.location.hostname.indexOf('tcard3.hyundaicard.com') >= 0) 
		&& typeof jQuery == 'undefined') {
	window.onload = onload_tracking;
} else {
	$(window).load(onload_tracking);
}
