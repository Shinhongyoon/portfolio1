CM_EVENT["TRME5"] = CM_EVENT["TRE5"];
CM_EVENT["TPME3"] = CM_EVENT["TPE3"];
TTD_IMAGEPIXEL["TRME5"] = TTD_IMAGEPIXEL["TRE5"];
TTD_IMAGEPIXEL["TPME3"] = TTD_IMAGEPIXEL["TPE3"];

function inno_track(key1, key2) {
	var trackingId = CM_EVENT[key1];
	if (trackingId != undefined && key2 != undefined) trackingId = trackingId[key2];
	if (trackingId != undefined && typeof gtag == 'function') {
		gtag('event', 'conversion', {
			'allow_custom_scripts': true,
			'send_to': 'DC-11856577/pagev0/' + trackingId + '+standard'
		});
	}
	if (trackingId != undefined && typeof wpts != 'undefined' && wpts.tag) {
		wpts.tag({
			ty : 'conversion',
			mud : {
				'allow_custom_scripts': true,
				'send_to': 'DC-11856577/pagev0/' + trackingId + '+standard'
			}
		});
	}
	trackingId = TTD_IMAGEPIXEL[key1];
	if (trackingId != undefined && key2 != undefined) trackingId = trackingId[key2];
	if (trackingId != undefined) 
		$(document.body).append('<img height="1" width="1" style="border-style:none;" alt="" src="https://insight.adsrvr.org/track/pxl/?adv=0krrt7u&ct=0:' + trackingId + '&fmt=3"/>');
}

